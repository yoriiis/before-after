{
	"name": "before-after",
	"version": "3.0.0",
	"description": "BeforeAfter is a lightweight Javascript library to compare images in before/after view without any dependencies",
	"keywords": [
		"jpeg mini",
		"hover",
		"animation",
		"slider",
		"before",
		"after",
		"before after",
		"simple",
		"customizable",
		"lightweight",
		"images"
	],
	"homepage": "https://github.com/yoriiis/before-after.js",
	"bugs": "https://github.com/yoriiis/before-after.js/issues",
	"repository": "git+https://github.com/yoriiis/before-after.js.git",
	"license": "MIT",
	"author": "Joris DANIEL",
	"main": "dist/before-after.js",
	"files": [
		"dist",
		"!dist/**/*.map"
	],
	"scripts": {
		"build": "echo \"No build specified\" && exit 0",
		"build:example": "rm -rf ./demo/dist/ && webpack --mode=production --config=./demo/webpack.config.js",
		"start:example": "rm -rf ./demo/dist/ && webpack serve --mode=development --config=./demo/webpack.config.js",
		"test": "npm run test:markdown && npm run test:eslint && npm run test:stylelint",
		"test:eslint": "eslint . --config ./config/.eslintrc.cjs",
		"test:markdown": "markdownlint-cli2 '**/*.md' '.github/*.md'",
		"test:stylelint": "stylelint \"./src/**/*.css\" \"./demo/**/*.css\" --config ./config/stylelint.config.cjs --fix"
	},
	"prettier": "./config/prettier.config.cjs",
	"browserslist": [
		"last 2 chrome version",
		"last 2 firefox version",
		"last 2 safari version",
		"last 2 opera version",
		"last 2 edge version",
		"last 2 ios_saf version"
	],
	"devDependencies": {
		"@babel/eslint-parser": "^7.21.8",
		"@babel/preset-env": "^7.22.2",
		"babel-loader": "^9.1.2",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"css-loader": "^6.8.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"html-webpack-plugin": "^5.5.1",
		"markdownlint-cli2": "^0.7.1",
		"mini-css-extract-plugin": "^2.7.5",
		"postcss": "^8.4.23",
		"postcss-custom-media": "^9.1.3",
		"postcss-import": "^15.1.0",
		"postcss-loader": "^7.2.4",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^8.3.2",
		"postcss-url": "^10.1.1",
		"prettier": "^2.8.7",
		"style-loader": "^3.3.3",
		"stylelint": "^14.10.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-standard": "^27.0.0",
		"stylelint-prettier": "^2.0.0",
		"terser-webpack-plugin": "^5.3.7",
		"webpack-cli": "^5.1.1",
		"webpack-dev-server": "^4.13.3"
	},
	"engines": {
		"node": ">=16.20.0"
	}
}
